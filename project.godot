; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="OpenPower"
run/main_scene="res://game/main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DBManager="*res://game/lib/autoload/db_manager.gd"
ECS="*res://addons/gecs/ecs.gd"

[easy_fullscreen]

enabled=true
action="fullscreen"

[editor_plugins]

enabled=PackedStringArray("res://addons/gdUnit4/plugin.cfg", "res://addons/gecs/plugin.cfg", "res://addons/godot-sqlite/plugin.cfg", "res://addons/json_editor/plugin.cfg")

[internationalization]

locale/translations_pot_files=PackedStringArray("res://main.tscn")

[maaacks_game_template]

disable_plugin_dialogues=true
copy_path="res://src"

[maaacks_menus_template]

disable_plugin_dialogues=true
copy_path="res://game/screens"
